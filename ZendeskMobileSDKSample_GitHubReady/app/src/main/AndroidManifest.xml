
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.atcltd.zendeskapp">
  <uses-permission android:name="android.permission.INTERNET"/>
  <application android:allowBackup="true" android:label="Zendesk Messenger" android:supportsRtl="true" android:theme="@style/Theme.Material3.DayNight.NoActionBar">
    <activity android:name="zendesk.messaging.android.MessagingActivity" />
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>
</manifest>
